          "example-1": {
            "cellNumber": "8475551212",
            "chatSessionId": "12345",
            "psapId": "23",
            "userId": "46",
            "carrierId": "2"
          }
        },
        "properties": {
          "cellNumber": {
            "type": "string",
            "description": "MDN for incoming transaction",
            "example": "8475551212"
          },
          "chatSessionId": {
            "type": "string",
            "description": "Session ID",
            "example": "12345"
          },
          "psapId": {
            "type": "string",
            "description": "PSAP ID (by AGENT511)",
