          },
          "userId": {
            "type": "string",
            "description": "PSAP queue ID (by AGENT511)",
            "example": "46"
          },
          "carrierId": {
            "type": "string",
            "description": "Carrier ID",
            "example": "2"
          },
          "dateTime": {
            "type": "string",
            "example": "2013-05-05 14:05:03:102",
            "description": "Transaction time on TEXTBLUE (GMT)"
          }
        },
        "required": [
          "cellNumber",
          "chatSessionId",
          "psapId",
          "userId",
          "dateTime"
        ]
      },
