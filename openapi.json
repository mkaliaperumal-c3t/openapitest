  "paths": {
    "/chat": {
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatSessionMT"
              },
              "examples": {
                "example-1": {
                  "value": {
                    "cellNumber": "8475551212",
                    "psapId": "23",
                    "userId": "46",
                    "msg": "This is test message"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CAPIResponse"
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "cellNumber": "123456",
                      "msgId": "1111"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        },
        "operationId": "createChatMTOp",
        "summary": "Initiate a new Chat",
        "description": "Initiate a MT Chat Session. ",
        "tags": [
          "chat"
        ]
      }
    },
    "/chat/{chatSessionId}/message": {
      "summary": "Messages related to the Chat Session",
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "allOf": [
                  {
                    "$ref": "#/components/schemas/ChatSessionMT"
                  },
                  {
                    "type": "object",
                    "properties": {
                      "language": {
                        "type": "string"
                      },
                      "translate": {
                        "type": "boolean"
                      }
                    }
                  }
                ]
              },
              "examples": {
                "example-1": {
                  "value": {
                    "cellNumber": "8475551212",
                    "psapId": "23",
                    "userId": "46",
                    "msg": "This is test message"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "examples": {
                  "example-1": {
                    "value": {
                      "cellNumber": "123456",
                      "msgId": "1111"
                    }
                  }
                },
                "schema": {
                  "$ref": "#/components/schemas/CAPIResponse"
                }
              }
            },
            "description": "Success Response retruned after sending the message to he reciepent"
          }
        },
        "operationId": "sendMessageOp",
        "summary": "Send Text Message on ongoing Chat",
        "tags": [
          "chat"
        ],
        "description": "Send Message"
      },
      "parameters": [
        {
          "name": "chatSessionId",
          "description": "Session Id",
          "schema": {
            "type": "string",
            "example": "12345"
          },
          "in": "path",
          "required": true
        }
      ]
    },
    "/chat/{chatSessionId}": {
      "summary": "Terminates the chat Session",
      "delete": {
        "parameters": [
          {
            "name": "cellNumber",
            "description": "MDN for the SMS recipient",
            "schema": {
              "type": "string",
              "example": "8475551212"
            },
            "in": "query",
            "required": true
          },
          {
            "name": "psapId",
            "description": "PSAP ID (by AGENT511)",
            "schema": {
              "type": "string",
              "example": "23"
            },
            "in": "query",
            "required": true
          },
          {
            "name": "userId",
            "description": "PSAP queue ID (by AGENT51)",
            "schema": {
              "type": "string",
              "example": "46"
            },
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "examples": {},
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/CAPIResponse"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "terminationCode": {
                          "type": "string"
                        }
                      }
                    }
                  ]
                }
              }
            },
            "description": "Success returns the message id of the terminated session send to the cellnumber"
          }
        },
        "summary": "Terminates the Chat Session",
        "tags": [
          "chat"
        ],
        "operationId": "deleteSessionOp",
        "description": "Terminates the Sesion"
      },
      "parameters": [
        {
          "schema": {
            "type": "string",
            "example": "12345"
          },
          "name": "chatSessionId",
          "in": "path",
          "description": "Session Id",
          "required": true
        }
      ]
    },
    "/chat/{chatSessionId}/location": {
      "summary": "Location for the cell number corresponding to the ChatSession",
      "get": {
        "parameters": [
          {
            "name": "cellNumber",
            "description": "MDN for the SMS recipient",
            "schema": {
              "type": "string",
              "example": "8475551212"
            },
            "in": "query",
            "required": true
          },
          {
            "name": "psapId",
            "description": "PSAP ID (by AGENT511)",
            "schema": {
              "type": "string",
              "example": "23"
            },
            "in": "query",
            "required": true
          },
          {
            "name": "userId",
            "description": "PSAP queue ID (by AGENT51)",
            "schema": {
              "type": "string",
              "example": "46"
            },
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Location"
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "longitude": "43.23434",
                      "accuracy": "100",
                      "latitude": "43.23434"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "ReBid/Identify the Location for the number corresponding to the ChatSession",
        "tags": [
          "chat"
        ],
        "operationId": "rebidOperation",
        "description": "Rebid Location"
      },
      "parameters": [
        {
          "schema": {
            "type": "string",
            "example": "12345"
          },
          "name": "chatSessionId",
          "in": "path",
          "required": true,
          "description": "Session ID "
        }
      ]
    },
    "/chat/{chatSessionId}/user": {
      "summary": "uses associated with the ChatSession",
      "post": {
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatSessionMT"
              },
              "examples": {
                "example-1": {
                  "value": {
                    "cellNumber": "8475551212",
                    "psapId": "23",
                    "userId": "46",
                    "msg": "This is test message"
                  }
                }
              }
            }
          },
          "required": true
        },
        "parameters": [
          {
            "name": "newPsapId",
            "description": "Transferring PSAP ID",
            "schema": {
              "type": "string",
              "example": "24"
            },
            "in": "query",
            "required": true
          },
          {
            "name": "newPsapUserId",
            "description": "Receiving PSAP ID",
            "schema": {
              "type": "string",
              "example": "47"
            },
            "in": "query",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "If transfer success return 200"
          }
        },
        "summary": "Transfer ChatSession to the given newpsapid/user",
        "tags": [
          "chat"
        ],
        "description": "Transfer Session to other user or psap",
        "operationId": "transferPsapUserOp"
      },
      "parameters": [
        {
          "name": "chatSessionId",
          "schema": {
            "type": "string",
            "example": "12345"
          },
          "in": "path",
          "required": true,
          "description": "Session ID "
        }
      ]
    },
    "/chat/{chatSessionid}/psap_external": {
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "chatSessionid",
          "in": "path",
          "required": true
        }
      ],
      "post": {
        "summary": "transferPsapExtOp",
        "tags": [
          "chat"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "operationId": "transferPsapExternalOp",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatSessionMT"
              }
            },
            "application/xml": {
              "schema": {
                "type": "object",
                "properties": {}
              }
            }
          }
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "fccId",
            "description": "FCC ID of the external psap  to transfer the chat session to",
            "required": true
          }
        ],
        "description": "Transfer the current session to External PSAP"
      }
    },
    "/chatSubscribe": {
      "post": {
        "summary": "Subscribe to Incoming Chat MO",
        "operationId": "chatSubscripeOp",
        "responses": {
          "201": {
            "description": "Created",
            "headers": {}
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "callBackUrl": {
                    "type": "string",
                    "description": "An URL in the API Client Environment",
                    "format": "uri",
                    "example": "https://clientServer.com/app/agent511"
                  }
                },
                "required": [
                  "callBackUrl"
                ]
              }
            }
          }
        },
        "description": "Subscribe for Chats or Events to be Published",
        "callbacks": {
          "ChatInitiated": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "New Call Initiated",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatInitiatedMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "New Chat Session has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatIncomingMsgReceived": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "New Incoming Message",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatIncomingMsgReceivedMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "New Message has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatLocationRebidMade": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "Rebid location only",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatLocationRebidMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "Rebid location details received",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatSessionExpired": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "Session expired",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatSessionExpiredMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "ChatSessionExpired has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatTransferMade": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "Transfer requested",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatTransferRequestedMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "ChatTransferMade has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatSystemMsgReceived": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "ChatSystemMsgReceived",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatSystemMsgMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "ChatSystemMsgReceived has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "ChatMMSRecieved": {
            "{$request.body#/callbackUrl}": {
              "post": {
                "summary": "ChatMMSReceived",
                "requestBody": {
                  "required": true,
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/ChatMMSReceivedMO"
                      }
                    }
                  }
                },
                "responses": {
                  "200": {
                    "description": "ChatMMSReceivedMO has been accepted",
                    "content": {
                      "text/plain": {
                        "examples": {
                          "ok": {
                            "value": "ok"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": [
          "hook"
        ]
      }
    },
    "/chat/{chatSessionId}/mms": {
